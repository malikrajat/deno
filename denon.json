       {
              "$schema": "https://deno.land/x/denon/schema.json",
              "scripts": {
                     "start": {
                            "cmd": "deno run main.ts",
                            "desc": "run my main.ts file",
                            "unstable": true,
                            "fmt": true,
                            "allow": [
                                   "run",
                                   "net",
                                   "read",
                                   "write",
                                   "plugin",
                                   "env"
                            ]
                     }
              },
              "logger": {
                     "fullscreen": false,
                     "quiet": false,
                     "debug": true
              }
       }